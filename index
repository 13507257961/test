<body>
	<div class="boxs">
		<div class="btns">
			<button class="open btn" id="open-up">
			点击弹出框
			</button>
		</div>
	</div>
	<div class="dialog-box" style="display:none;"></div>
</body>
<script src="js/jquery-1.8.2.min.js"></script>
<script src="js/popBox.js"></script>
<script>
	$(function(){
		$("#open-up").click(function(){
				var $d = $(".dialog-box");
			$d.dialog_1({
				title:"登陆", 	//标题
				dragable:true,  //状态
				html:'',  		//html 模版
				width:400,      //宽度
				height:200,    //高度
				cannelText:'取消', //取消text
				confirmText:"确认", //确认text
				showFooter:true, 
			}).open();
		});
	});
</script>
